<bindings xmlns="http://java.sun.com/xml/ns/jaxb"
          xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          version="2.1">
    <bindings schemaLocation="collada_schema.xml" version="1.0">
        <!-- rename the value element -->
        <bindings node="//xs:complexType[@name='skin_type']">
            <bindings node=".//xs:sequence/xs:element[@name='source']">
                <property name="SkinSource"/>
            </bindings>
        </bindings>
        <bindings node="//xs:complexType[@name='morph_type']">
            <bindings node=".//xs:sequence/xs:element[@name='source']">
                <property name="SkinSource"/>
            </bindings>
        </bindings>
    </bindings>
</bindings>